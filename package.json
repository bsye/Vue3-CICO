{
  "name": "vue-cico",
  "version": "2.1.0",
  "description": "Smartbox version of check-in check-out",
  "author": "Smartbox",
  "main": "dist/vueCico.common.js",
  "repository": {
    "url": "git@gitlab.production.smartbox.com:npm/vue-cico.git",
    "type": "git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "date",
    "dates",
    "input",
    "date input",
    "datepicker",
    "date picker",
    "range",
    "date range picker",
    "vue-datepicker",
    "vue-hotel-datepicker",
    "vue-date-range"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "test:unit": "vitest run --coverage",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "components/*.vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.1",
    "fecha": "^4.2.1",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "vite": "^4.3.3",
    "vue": "^3.1.0-0"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/compiler-sfc": "^3.1.0-0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "^9.10.5",
    "jest": "^27.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "vitest": "^0.30.1"
  }
}
